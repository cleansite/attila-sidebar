<ul class="nav-meta">
  {{#if @custom.twitter}}
  <li class="nav-twitter">
    <a aria-label="Twitter" href="https://twitter.com/{{@custom.twitter}}" target="_blank">
      <i class="icon icon-twitter" aria-hidden="true"></i>
      <span>{{@custom.twitter}}</span>
    </a>
  </li>
  {{/if}}
    {{#if @custom.linkedin}}
  <li class="nav-linkedin">
    <a aria-label="LinkedIn" href="https://www.linkedin.com/in/{{@custom.linkedin}}" target="_blank">
      <i class="icon icon-linkedin" aria-hidden="true"></i>
      <span>{{@custom.linkedin}}</span>
    </a>
  </li>
  {{/if}}
  {{#if @custom.facebook}}
  <li class="nav-facebook">
    <a aria-label="Facebook" href="https://facebook.com/{{@custom.facebook}}" target="_blank">
      <i class="icon icon-facebook" aria-hidden="true"></i>
      <span>{{@custom.facebook}}</span>
    </a>
  </li>
  {{/if}}
  {{#if @custom.instagram}}
  <li class="nav-instagram">
    <a aria-label="Instagram" href="https://instagram.com/{{@custom.instagram}}" target="_blank">
      <i class="icon iconnew-instagram" aria-hidden="true"></i>
      <span>{{@custom.instagram}}</span>
    </a>
  </li>
  {{/if}}
  {{#if @custom.behance}}
  <li class="nav-behance">
    <a aria-label="behance" href="https://www.behance.net/{{@custom.behance}}" target="_blank">
      <i class="icon iconnew-behance" aria-hidden="true"></i>
      <span>{{@custom.behance}}</span>
    </a>
  </li>
  {{/if}}
</ul>
